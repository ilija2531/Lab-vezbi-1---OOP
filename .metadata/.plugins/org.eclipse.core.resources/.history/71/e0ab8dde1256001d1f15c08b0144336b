package vezba5;

public class Vezba5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String unit = "степен";
		String value = "2.5";
		double temperature = Double.parseDouble(value);
		System.out.println("Temperaturata iznesuva " + (int)(temperature * 10) + unit);

	}

}
